<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/itemNomeTelaPizza"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/itemId"
        android:layout_alignLeft="@+id/itemTelefone"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/itemId"
        android:layout_alignStart="@+id/itemTelefone"
        android:layout_marginTop="12dp"
        android:text="nome"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.037"
        tools:ignore="HardcodedText,RtlHardcoded" />

    <TextView
        android:id="@+id/itemTelefone"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="13dp"
        android:layout_marginStart="13dp"
        android:text="telefone"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/itemId"
        app:layout_constraintTop_toBottomOf="@+id/itemNomeTelaPizza"
        app:layout_constraintVertical_bias="0.008"
        tools:ignore="HardcodedText,RtlHardcoded"
        android:layout_marginTop="11dp"
        android:layout_below="@+id/itemNomeTelaPizza"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/itemId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="11dp"
        android:layout_marginRight="11dp"
        android:text="itemId"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="@+id/itemNomeTelaPizza"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/itemNomeTelaPizza"
        app:layout_constraintVertical_bias="0.008"
        tools:ignore="HardcodedText,RtlHardcoded"
        android:layout_alignTop="@+id/itemTelefone" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/itemTelefone"
        android:layout_alignStart="@+id/itemTelefone"
        android:layout_below="@+id/itemTelefone"
        tools:ignore="RtlHardcoded" />

</RelativeLayout>